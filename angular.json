{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "TuZonaSegura": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "schematics": { "@schematics/angular:application": { "strict": true } },
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "ngx-build-plus:browser",
          "options": {
            "commonChunk": false,
            "outputPath": "dist/TuZonaSegura",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.app.json",
            "aot": true,
            "assets": [
              { "glob": "favicon.ico", "input": "src", "output": "/" },
              { "glob": "**/*", "input": "src/assets", "output": "/assets" },
              { "glob": "**/*", "input": "src/img", "output": "/img" }
            ],
            "styles": [
              "src/assets/css/bootstrap.min.css",
              "src/assets/css/accordion.css",
              "node_modules/@swimlane/ngx-datatable/index.css",
              "node_modules/@swimlane/ngx-datatable/themes/material.scss",
              "node_modules/@swimlane/ngx-datatable/assets/icons.css",
              "node_modules/@fortawesome/fontawesome-free/css/all.css",
              "src/styles.css",
              "src/assets/css/print.css"
            ],
            "scripts": [
              "src/assets/js/jquery-3.5.1.js",
              "src/assets/js/menu.js",
              "src/assets/js/header.js",
              "src/assets/js/popper.min.js",
              "src/assets/js/bootstrap.min.js"
            ]
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "4mb",
                  "maximumError": "4mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "4mb",
                  "maximumError": "4mb"
                }
              ],
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": true,
              "optimization": true,
              "vendorChunk": false,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          }
        },
        "serve": {
          "builder": "ngx-build-plus:dev-server",
          "options": { "browserTarget": "TuZonaSegura:build" },
          "configurations": {
            "production": { "browserTarget": "TuZonaSegura:build:production" }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": { "browserTarget": "TuZonaSegura:build" }
        },
        "test": {
          "builder": "ngx-build-plus:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "src/tsconfig.spec.json",
            "karmaConfig": "src/karma.conf.js",
            "styles": [{ "input": "styles.css" }],
            "scripts": [],
            "assets": [
              { "glob": "favicon.ico", "input": "src/", "output": "/" },
              { "glob": "**/*", "input": "src/assets", "output": "/assets" }
            ]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["src/tsconfig.app.json", "src/tsconfig.spec.json"],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "TuZonaSegura-e2e": {
      "root": "e2e/",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "TuZonaSegura:serve"
          }
